  "dependencies": {
    "async": "^2.5.0",
    "data-expression": "1.0.0",
    "glob": "^7.1.2",
    "joi": "^11.0.3",
    "jsdom": "11.2.0",
    "random-lib": "2.1.0",
    "tldjs": "2.2.0",
    "underscore": "1.8.3",
    "underscore.string": "3.3.4"
